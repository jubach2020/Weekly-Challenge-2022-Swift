#include <iostream>
#include <string.h>
using namespace std;
int main(void){
    int numero=100;
    string resultado;
    
    cout << numero << endl;
    do {
        resultado=resultado+std::to_string(numero%2);
        numero=numero/2;
    } while (numero>0);
    string iresult(resultado.rbegin(), resultado.rend());
    
    cout << iresult << endl;
}
